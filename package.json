{
  "name": "odata-demo-search",
  "version": "1.0.0",
  "description": "A full-stack application demonstrating OData service integration with search functionality",
  "main": "index.js",
  "scripts": {
    "start:backend": "cd odata-proxy-backend && node server.js",
    "start:frontend": "cd frontend && npm start",
    "install:backend": "cd odata-proxy-backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "deploy:setup": "npm run install:backend && npm run install:frontend"
  },
  "keywords": [
    "odata",
    "northwind",
    "search",
    "nodejs",
    "express",
    "proxy"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
